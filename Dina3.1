#include <stdio.h>
#include <math.h>

double f1(double x){
    return 8*x*x*x*cos(x);
    }

double f2(double x){
    return log(1+sqrt(x)-cos(x));
}

int main()
{ 
    double x=0, y, h;
    int a,b;
    printf("Шаг - ");
    scanf("%lf", &h);
    a = 2/h;
    b = 0;
    
    do 
    { 
  
        b += 1;
        if (b<(a/2)){
            y = f1(x);
        }
        else {
            y = f2(x);
        }
        printf("%f %f\n", x, y);    
        x = x + h;    
    
    
    } while (b<=a);
    
}
